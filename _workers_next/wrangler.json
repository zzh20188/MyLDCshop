{
    "name": "ldc-shop-next",
    "main": ".open-next/worker.js",
    "compatibility_date": "2025-11-12",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    "observability": {
        "enabled": true
    },
    "routes": [
        {
            "pattern": "shop.example.com",
            "custom_domain": true
        }
    ],
    "vars": {
        "NEXT_PUBLIC_APP_URL": "https://shop.example.com"
    }
}